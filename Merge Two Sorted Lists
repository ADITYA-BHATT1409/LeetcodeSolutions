class Solution {
    public ListNode mergeTwoLists(ListNode list1, ListNode list2) {
        if(list1==null){
            return list2;
        }
        if(list2==null){
            return list1;
        }
        ListNode head=null;
        ListNode dummy=null;
        if(list1.val>list2.val){
            head=list2;
            dummy=(mergeTwoLists(list1,list2.next));
        }else{
            head=list1;
            dummy=(mergeTwoLists(list1.next,list2));

        }
        head.next=dummy;
        return head;
        
    }
}
