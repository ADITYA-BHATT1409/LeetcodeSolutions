class Solution {
    public int[] sumZero(int n) {
        int []array=new int[n];
        int count=n;
        for(int i=0;i<n;i++){
            array[i]=-1*count;
            array[n-1-i]=count;
            count--;

        }
        if (n%2==1){
            array[n/2]=0;
        }
            
        return array;
        
    }
}
