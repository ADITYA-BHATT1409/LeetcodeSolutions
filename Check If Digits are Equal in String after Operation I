class Solution {
    public boolean hasSameDigits(String s) {
        int n=s.length();
        while (n>2) {
            StringBuilder sb=new StringBuilder();
            for (int i=0;i+1<n;i++) {
                int sum=(s.charAt(i) -'0')+(s.charAt(i+1)-'0');
                sb.append(sum%10);
            }
            s=sb.toString();
        }
        return s.charAt(0)==s.charAt(1);
    }
}
