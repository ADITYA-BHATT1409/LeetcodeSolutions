class Solution {
    public int missingNumber(int[] nums) {
        int n=nums.length;
        int[]Hash=new int[n+1];
        for (int i=0;i<n;i++){
            Hash [nums[i]]=1;
        }
        int result=0;
        for (int i=0;i<n+1;i++){
            if (Hash[i]==0){
                result=i;
                break;
            }
        }
        return result;
    }
}
