class Solution {
    public ListNode modifiedList(int[] nums, ListNode head) {
        HashSet<Integer> excludeSet = new HashSet<>();
        for (int val : nums) {
            excludeSet.add(val);
        }
        ListNode dummy = new ListNode(0);
        dummy.next = head;
        ListNode curr = dummy;
        while (curr.next != null) {
            if (excludeSet.contains(curr.next.val)) {
                curr.next = curr.next.next; 
            } else {
                curr = curr.next;
            }
        }
        return dummy.next;
        
    }
}
