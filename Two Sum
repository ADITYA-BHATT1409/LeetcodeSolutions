class Solution {
    public int[] twoSum(int[] nums, int target) {
        int n=nums.length;
        HashMap<Integer,Integer>map=new HashMap<>();
        for(int i=0;i<=n-1;i++){
            int num=nums[i];
            int diff=target-num;
            if (map.containsKey(diff)){
                return new int[]{map.get(diff),i};
            }
            map.put(num,i);
        }
        return new int[]{};
    }
}
