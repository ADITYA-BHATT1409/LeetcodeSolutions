class Solution {
    public int majorityElement(int[] nums) {
        int n=nums.length;
        HashMap<Integer,Integer>map=new HashMap<>();
        for (int i=0;i<=n-1;i++){
            map.put(nums[i],map.getOrDefault(nums[i],0)+1);
        }
        int result=-1;
        int maxval=0;
        for(int i:map.keySet()){
            if(map.get(i)>maxval){
                maxval=map.get(i);
                result=i;
            }
        }
        return result;    
    }
}
