class Solution {
    public int maxProfit(int[] prices) {
        int n=prices.length;
        int buyp=prices[0];
        int maxp=0;
        for(int i=0; i<n;i++){
            if (buyp>prices[i]){
                buyp=prices[i];
            }else{
                int cp=prices[i]-buyp;
                maxp=Math.max(maxp,cp);
            }
        }
        return maxp;
    }
}
